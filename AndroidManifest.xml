    package="com.niklashalle.muffin20.activities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RuleOfThreeActivity" />
        <activity android:name=".TimerActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".games.GameHubActivity" />
        <activity android:name=".games.TicTacToeActivity" />
        <activity android:name=".games.TicTacToeSettings" />
    </application>

</manifest>